{
  "name": "browserizr",
  "version": "3.2.1",
  "description": "Browserizr is tiny library, that detects your browser and platform, with testing window.navigator.userAgent and window.navigator.platform.",
  "main": "index.js",
  "scripts": {
    "build": "npm test && npm run dist && npm run eslint-dist",
    "test": "npm run eslint && npm run jest",
    "jest": "echo \"no jest\"",
    "dist": "npm run rollup && npm run uglify && npm run eslint-dist",
    "rollup": "rollup -c",
    "uglify": "uglifyjs -cmo dist/browserizr.min.js -- dist/browserizr.js",
    "eslint": "eslint \"./index.js\" --fix --parser babel-eslint --parser-options \"ecmaVersion: 6\" --cache --cache-location ./.cache/.eslintcache",
    "eslint-dist": "eslint \"./dist/browserizr.js\" --fix --rule \"no-unused-vars: 0, no-mixed-operators: 0\" --parser-options \"ecmaVersion: 5\" --cache --cache-location ./.cache/.eslintcache-dist",
    "serve": "browser-sync start -s --open external"
  },
  "jest": {
    "verbose": true,
    "cacheDirectory": "./.cache/jest/",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WezomAgency/browserizr.git"
  },
  "keywords": [
    "browser",
    "js",
    "detect",
    "userAgent",
    "platform",
    "android",
    "android3",
    "android4",
    "android5",
    "android6",
    "android7",
    "android8",
    "chrome",
    "edge",
    "edge-android",
    "edge-ios",
    "ie",
    "ie8",
    "ie9",
    "ie10",
    "ie11",
    "ios",
    "ipad",
    "ipod",
    "iphone",
    "iphone4",
    "iphone5",
    "iphone6",
    "iphone7",
    "iphone8",
    "iphonex",
    "linux",
    "mac",
    "maclike",
    "meizu-phone",
    "meizunote-phone",
    "mozilla",
    "firefox",
    "opera",
    "redmi-phone",
    "redminote-phone",
    "safari",
    "windows",
    "windows-xp",
    "windows-vista",
    "windows7",
    "windows8",
    "windows10",
    "windows-phone"
  ],
  "author": "Oleg Dutchenko <dutchenko.o.dev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WezomAgency/browserizr/issues"
  },
  "homepage": "https://github.com/WezomAgency/browserizr#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.3.0",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "rollup": "^0.63.5",
    "rollup-plugin-babel": "^3.0.7",
    "uglify-js": "^3.4.6"
  }
}
